{
  "name": "server",
  "version": "1.0.0",
  "main": "./dist/server.js",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --transpile-only --ignore-watch node_modules --respawn src/server.ts",
    "knex:migrate": "cross-env NODE_ENV=production knex --knexfile knexfile.ts migrate:latest",
    "knex:migrate:rollback": "knex --knexfile knexfile.ts migrate:rollback",
    "test": "cross-env NODE_ENV=test jest",
    "dist": "cross-env NODE_ENV=production tsc",
    "build": "tsc"
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/mongoose": "^5.7.37",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.4",
    "@types/multer-s3": "^2.7.8",
    "@types/node": "^14.14.7",
    "@types/yup": "^0.29.8",
    "cross-env": "^7.0.2",
    "pg": "^8.4.2",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "aws-sdk": "^2.787.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.21.5",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "sqlite3": "^5.0.0",
    "yup": "^0.29.3"
  },
  "build": {
    "files": [
      "dist/"
    ]
  }
}
